{
    "name": "deeparteffects/sdk-php",
    "description": "Deep Art Effects SDK for PHP",
    "keywords": [
        "deeparteffects",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://developer.deeparteffects.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Deep Art Effects GmbH",
            "homepage": "https://github.com/deeparteffects/sdk-php"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "aws/aws-sdk-php": "^3.28"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Deeparteffects\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Deeparteffects\\Client\\" : "test/" }
    }
}
